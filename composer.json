{
    "name": "livewire/livewire",
    "description": "Build and test rich client-side functionality: with only backend code.",
    "license": "MIT",
    "authors": [
        {
            "name": "Caleb Porzio",
            "email": "calebporzio@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "illuminate/validation": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "symfony/http-kernel": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Livewire\\LivewireServiceProvider"
            ],
            "aliases": {
                "Livewire": "Livewire\\Livewire"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Livewire\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "vendor/orchestra/testbench-core/laravel/app"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.15|^8.4",
        "laravel/framework": "~5.6.0|~5.7.0|~5.8.0|^6.0",
        "orchestra/testbench": "^3.8|^4.0",
        "psy/psysh": "@stable"
    }
}
